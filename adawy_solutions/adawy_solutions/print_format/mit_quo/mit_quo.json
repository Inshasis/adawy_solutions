 "html": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css\">\r\n    <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js\"></script>\r\n    <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js\"></script>\r\n    <style>\r\n        table {\r\n          font-family: arial, sans-serif;\r\n          border-collapse: collapse;\r\n        }\r\n        td, th {\r\n          border-top: 1px solid black;\r\n          text-align: left;\r\n        }\r\n        .tablee td, th {\r\n            width: 0%;\r\n        }\r\n        </style>    \r\n</head>\r\n<body >\r\n\t<div class=\"letter\" style=\"margin:-20px;\">\r\n\t    <div class=\"header\">\r\n              \r\n                <!--<div class=\"main1\" style=\"width: 100%;float: left;text-align: center;font-weight: 700;border-top : 1px solid black;border-bottom : 1px solid black;\">-->\r\n                <!--    <div class=\"pra3\" style=\"width: 56%;float: left;text-align: right;\">-->\r\n                <!--        <p style=\"font-size:15px;\">TAX INVOICE</p>-->\r\n                <!--    </div>-->\r\n                    \r\n                <!--</div>-->\r\n                <img style=\"margin-top:20px;\" src=\"/files/Letter MIT-01.png\">\r\n                <br>\r\n                <center><p style=\"font-size:15px;font-weight: 700;float: center;padding-top: 10px !important;margin-top:-15px;\">TAX INVOICE (OM17105475)</p></center>\r\n                <br> \r\n                <div class=\"main2\" style=\"font-weight: 600;width: 98%;float: left;padding-left: 20px;padding-top: 0px !important;\">\r\n                    <div class=\"pra5\" style=\"width: 50%; float: left;font-weight: 600;padding-bottom:-10px;border-top:1px solid;border-right:1px solid;border-left:1px solid;padding-left: 10px;\">\r\n                        <h5 style=\"font-size: 14px;\"><b>Customer</b></h5>\r\n                        <p style=\"font-size:13px;font-weight: 700;\">{{doc.party_name}}</p>\r\n                        {% if doc.contact_person %}\r\n                        {% set first_name,mobile_no,email_id = frappe.db.get_value('Contact', {'name':doc.contact_person}, ['first_name','mobile_no','email_id']) %}\r\n                        <p style=\"font-size:12px;font-weight: 700;\">Contact person: {{first_name or ''}}</p>\r\n                        <p style=\"font-size:12px;font-weight: 700;\">Mobile no: {{mobile_no or ''}}</p>\r\n                        <!--<p style=\"font-size:12px;font-weight: 700;\">Email: {{email_id or ''}}</p>-->\r\n                        {% endif %}\r\n                        \r\n                        {% if doc.customer_address %}\r\n                        {% set address_line1,address_line2,city,country,state = frappe.db.get_value('Address', {'name':doc.customer_address}, ['address_line1','address_line2','city','country','state']) %}\r\n                        <p style=\"font-size:11px;\">Address : {{address_line1 or ''}}, {{address_line2 or ''}}, {{city or ''}}, {{state or ''}},{{country or ''}} </p>\r\n                        {% else %}\r\n                        <p style=\"font-size:11px;\">Address :</p>\r\n                        {% endif %}\r\n                        {% set tax_id = frappe.db.get_value('Customer', {'name':doc.party_name}, ['tax_id']) %}\r\n                        \r\n                        <!--<p style=\"font-size:12px;font-weight: 700;\">Location:</p>-->\r\n                        <!--<p style=\"font-size:11px;;\">VAT No : {{tax_id or ''}}</p>-->\r\n                    \r\n                        <p style=\"font-size:11px;height:10px;\"></p>\r\n                        \r\n                    </div>\r\n                    \r\n                    <div class=\"pra5\" style=\"width: 49.7%; float: left;font-weight: 600;padding-bottom:-10px;border-top:1px solid;border-right:1px solid;padding-left: 10px;\">\r\n                        <h5 style=\"font-size: 14px;\"><b>Quotation Details</b></h5>\r\n                        <p style=\"font-size:11px;font-weight: 600;\">Quotation No:  {{doc.name}}</p>\r\n                        <p style=\"font-size:11px;font-weight: 600;\">Quotation Date: {{ frappe.format(doc.transaction_date, {'fieldtype': 'Date'}) }}</p>\r\n                        <p style=\"font-size:11px;font-weight: 600;\">Currency : {{doc.currency or ''}}</p>\r\n                        <p style=\"font-size:11px;font-weight: 600;\">Lead : </p>\r\n                        <p style=\"font-size:11px;height:17px;\"></p>\r\n                        \r\n                    </div>\r\n                </div>\r\n                \r\n                <!--<div class=\"main2\" style=\"font-weight: 600;width: 97.7%;float: left;padding-left: 20px;\">-->\r\n                <!--    <div class=\"pra5\" style=\"width: 100%; float: left;padding-left: 10px;border-top:1px solid;border-right:1px solid black;border-left:1px solid black;padding-left: 10px;\">-->\r\n                <!--        <h5 style=\"font-size: 14px;\"><b>Bill Buy</b></h5>-->\r\n                <!--        {% set user=frappe.session.user %}-->\r\n                <!--        {% set email,mobile_no,phone = frappe.db.get_value('User', {'name':user}, ['email','mobile_no','phone']) %}-->\r\n                <!--        <p style=\"font-size:11px;font-weight: 700;\">Username : {{user or ''}} </p>-->\r\n                <!--        <p style=\"font-size:11px\">Department : </p>-->\r\n                <!--        <p style=\"font-size:11px;margin-left:400px;margin-top:-37px;\">E-Mail : {{email or ''}} </p>-->\r\n                <!--        <p style=\"font-size:11px;margin-left:400px;margin-top:-0px;height:20px;\">Landline : {{phone or ''}}</p>-->\r\n                        <!--<p style=\"font-size:11px\">Mobile No : {{mobile_no or ''}}</p>-->\r\n                        <!--<p style=\"font-size:11px;height:20px;\"></p>-->\r\n                    \r\n                <!--    </div>-->\r\n                    \r\n                <!--</div>-->\r\n                \r\n                <div class=\"main4\">\r\n                    <table class=\"table\" style=\"border-top: 1px solid black;border-bottom: 1px solid black;border-left: 1px solid black;width: 95%;margin-left: 20px;margin-top: !important;padding: 48px;\">\r\n                        <thead>\r\n                        <tr style=\"background-color: lightgray;font-size:12px;\">\r\n                            <th style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:5%;\"><b>#</b></th>\r\n                            <th style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:40%\"><b>Item Description</b></th>\r\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:5%\"><b>Operator</b> </th>-->\r\n                            <th style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:15%\"><b>U.O.M</b></th>\r\n                            <th style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:10%\"><b>Qty</b></th>\r\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:8%\"><b>Rate</b></th>-->\r\n                            <!-- <th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:8%\"><b>Amount</b></th>-->\r\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:6%\"><b>VAT (%)</b></th>-->\r\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:10%\"><b>VAT Amount</b></th>-->\r\n                            <!--<th style=\"border-right:1px solid black ;border-bottom:1px solid black!important;color:black !important;width:12%\"><b>Total Amount</b></th>-->\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {% for i in doc.items %}\r\n                        <tr style=\"height: 27px;\">\r\n                            <td style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black ;\">{{i.idx}}</td>\r\n                            <td style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black ;\"><b>{{i.item_name}}</b> ({{i.item_code}})</td>\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{i.operator}}</td>-->\r\n                            <td style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black ;\">{{i.uom}}</td>\r\n                            <td style=\"text-align: center;border-right:1px solid black ;border-bottom:1px solid black ;\">{{i.qty}}</td>\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{\"%.3f\"|format(i.rate) }}</td>-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{\"%.3f\"|format(i.amount) }}</td>-->\r\n                            <!--{% if doc.tax_category == 'VAT 5%' %}-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">5%</td>-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{\"%.3f\"|format(i.amount*5/100)}}</td>-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{\"%.3f\"|format(i.amount + i.amount*5/100)}}</td>-->\r\n                            <!--{% else %}-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">0%</td>-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">0.000</td>-->\r\n                            <!--<td style=\"border-right:1px solid black ;border-bottom:1px solid black ;\">{{\"%.3f\"|format(i.amount)}}</td>-->\r\n                            {% endif %}\r\n                        </tr>\r\n                        {% endfor %}\r\n                    </tbody>\r\n                    <!--<tr>-->\r\n                    <!--   <td colspan=\"3\" style=\"border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: right;font-size:14px;\">Sub Total</td>-->\r\n                    <!--   <td style=\"border-right:1px solid black;border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: center;font-size:14px;\">{{doc.base_total}}</td>-->\r\n                    <!--</tr>-->\r\n                    \r\n                    <!-- <tr>-->\r\n                    <!--   <td colspan=\"3\" style=\"border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: right;font-size:14px;\">Discount ({{doc.additional_discount_percentage}}%)</td>-->\r\n                    <!--   <td style=\"border-right:1px solid black;border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: center;font-size:14px;\">{{ \"%.2f\"|format(doc.discount_amount ) }}</td>-->\r\n                    <!--</tr>-->\r\n                    \r\n                    <!-- <tr>-->\r\n                    <!--   <td colspan=\"3\" style=\"border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: right;font-size:14px;\">VAT (5%)</td>-->\r\n                    <!--   <td style=\"border-right:1px solid black;border-bottom:1px solid black ;border-left:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: center;font-size:14px;\">{{ \"%.2f\"|format(doc.total_taxes_and_charges) }}</td>-->\r\n                    <!--</tr>-->\r\n                    \r\n                    <!-- <tr>-->\r\n                    <!--   <td colspan=\"3\" style=\"border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: right;font-size:14px;\">Grand Total</td>-->\r\n                    <!--   <td style=\"border-right:1px solid black;border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: center;font-size:14px;\">{{ \"%.2f\"|format(doc.grand_total) }}</td>-->\r\n                    <!--</tr>-->\r\n                    \r\n                    <!--<tr>-->\r\n                       <!--<td colspan=\"1\" style=\"border-left:1px solid black ;border-bottom:1px solid black ;background-color:#F8F8F7;font-weight:700;text-align: right;font-size:14px;\">In Word</td>-->\r\n                    <!--   <td colspan=\"4\" style=\"border-left:1px solid black ;border-right:1px solid black ;border-bottom:1px solid black ;background-color:#FFF0DA;font-weight:700;text-align: right;font-size:14px;\">{{doc.in_words}}</td>-->\r\n                    <!--</tr>-->\r\n                </table>\r\n            </div>\r\n            \r\n                <div class=\"main2\" style=\"font-weight: 600;width: 98%;float: left;padding-left: 20px;padding-top: 0px !important;margin-top:-20px;\">\r\n                    <div class=\"pra5\" style=\"width: 64%; font-size: 12px;float: left;font-weight: 600;padding-bottom:-10px;border-bottom:1px soli